	<remote name="oe" 			fetch="https://github.com/openembedded"/>
		<project remote="oe" revision="jethro" name="meta-openembedded" path="sources/meta-openembedded"/>		
		<project remote="oe" revision="jethro" name="oe-core" path="sources/oe-core"/>
		<project remote="oe" revision="jethro" name="meta-oe" path="sources/meta-oe"/>


	<remote name="toradex"		fetch="http://git.toradex.com"/>
		<project remote="toradex" revision="V2.6" name="meta-toradex" path="sources/meta-toradex">

	<remote name="qt5"			fetch="http://github.com/meta-qt5"/>
