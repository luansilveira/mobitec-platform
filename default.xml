<?xml version="1.0" encoding="UTF-8"?>
<manifest>

	<default sync-j="4" revision="jethro"/>

	<remote name="yocto" 		fetch="https://git.yoctoproject.org/git"/>
	<remote name="freescale" 	fetch="https://github.com/freescale"/>
	<remote name="openembedded" 	fetch="https://github.com/openembedded"/>
	<remote name="toradex" 		fetch="http://git.toradex.com"/>
	<remote name="qt5" 		fetch="http://github.com/meta-qt5"/>

	<project name="poky.git" 		remote="yocto" revision="jethro" path="sources/poky"/>
	<project name="meta-fsl-arm.git" 	remote="yocto" revision="jethro" path="sources/meta-fsl-arm"/>

	<project name="fsl-community-bsp-base.git" remote="freescale" revision="jethro" path="sources/base">
		<copyfile dest="README" src="README"/>
		<copyfile dest="setup-environment" src="setup-environment"/>
	</project>

	<project name="meta-fsl-arm-extra.git" 	remote="freescale" revision="jethro" path="sources/meta-fsl-arm-extra"/>
	<project name="meta-fsl-demos.git" 	remote="freescale" revision="jethro" path="sources/meta-fsl-demos"/>
	<project name="Documentation.git" 	remote="freescale" revision="jethro" path="sources/meta-fsl-docs"/>	

	<project name="meta-openembedded.git" 	remote="openembedded" revision="jethro" path="sources/meta-openembedded"/>		
	<project name="openembedded-core.git" 	remote="openembedded" revision="jethro" path="sources/openembedded-core"/>

	<project name="meta-toradex.git" 	remote="toradex" revision="V2.6" path="sources/meta-toradex"/>

	<project name="meta-qt5.git" 		remote="qt5" revision="jethro" path="sources/meta-qt5"/> 

</manifest>
